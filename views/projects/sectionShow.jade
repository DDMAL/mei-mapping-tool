    
div.boxSection
		p #{section.name}
		ul
			- each neume in neumes
				p(id = "showNeume#{neume.id}" hidden) #{neume.neumeSection}

				script.
					if("#{neume.neumeSection}" == "#{section.id}"){
					document.getElementById("showNeume#{neume.id}").style.display = "block";

